{
  "name": "@garystorey/slugify",
  "version": "1.0.1",
  "description": "Utility to remove whitepace from a string and replace it with a dash",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "eslint --fix \"./src/index.ts\"",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "whitespace-remove",
    "slug",
    "slugify"
  ],
  "author": "Gary Storey <garystorey655@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garystorey/slugify/issues"
  },
  "homepage": "https://github.com/garystorey/slugify#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4",
    "ts-jest": "^26.5.6"
  }
}
